server.port=<?server-port?>
logging.level.root=INFO
logging.file=ar4k.log
ar4k.test=false
ar4k.confPath=~/.ar4k
ar4k.fileConfig=
ar4k.webConfig=
ar4k.dnsConfig=
ar4k.baseConfig=<?base64-config?>
ar4k.fileKeystore=<?keystore-file?>
ar4k.webKeystore=
ar4k.dnsKeystore=
ar4k.keystoreMainAlias=<?keystore-ca?>
ar4k.keystoreBeaconAlias=<?keystore-beacon?>
ar4k.keystorePassword=<?keystore-password?>
ar4k.beaconCaChainPem=
ar4k.adminPassword=<?admin-password?>
ar4k.webRegistrationEndpoint=<?beacon-endpoint?>
ar4k.dnsRegistrationEndpoint=
ar4k.beaconDiscoveryFilterString=AR4K
ar4k.beaconDiscoveryPort=<?discovery-port?>
ar4k.fileConfigOrder=1
ar4k.webConfigOrder=2
ar4k.dnsConfigOrder=3
ar4k.baseConfigOrder=0
ar4k.threadSleep=1500
ar4k.logoUrl=/static/img/ar4k.png
ar4k.consoleOnly=false
management.endpoints.web.base-path=/control
management.endpoints.default.web.enabled=true
management.endpoints.default.jmx.enabled=false
management.endpoint.health.show-details=always
management.endpoints.web.exposure.include=*
info.app.name=Ar4k Agent
info.app.description=Ar4k IoT Cloud Gateway
management.info.git.mode=full
camel.springboot.xmlRoutes=false
spring.main.allow-bean-definition-overriding=true